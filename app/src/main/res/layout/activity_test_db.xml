<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".testDB">

    <EditText
        android:id="@+id/latTex"
        android:layout_width="173dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:hint="lat"
        android:inputType="numberDecimal"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editior_absoluteX="84dp"
        tools:layout_editior_absoluteY="16dp"
        tools:layout_editor_absoluteY="30dp" />

    <EditText
        android:id="@+id/lngTex"
        android:layout_width="176dp"
        android:layout_height="46dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="lng"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editior_absoluteX="147dp"
        tools:layout_editior_absoluteY="16dp"
        tools:layout_editor_absoluteY="30dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Save Data"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editior_absoluteX="210dp"
        tools:layout_editior_absoluteY="92dp" />

    <TextView
        android:id="@+id/latView"
        android:layout_width="171dp"
        android:layout_height="43dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="101dp" />

    <TextView
        android:id="@+id/lngView"
        android:layout_width="173dp"
        android:layout_height="37dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="153dp" />


</android.support.constraint.ConstraintLayout>